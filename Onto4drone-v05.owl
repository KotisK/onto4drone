<?xml version="1.0"?>
<rdf:RDF xmlns="http://i-lab.aegean.gr/kotis/ontologies/onto4drone"
     xml:base="http://i-lab.aegean.gr/kotis/ontologies/onto4drone"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:vann="http://purl.org/vocab/vann/">
    <owl:Ontology rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone">
        <owl:versionIRI rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone/1.0.0"/>
        <owl:imports rdf:resource="http://www.datacron-project.eu/datAcron#"/>
        <dc:contributor>A. Soularidis, i-Lab, University of the Aegean</dc:contributor>
        <dc:contributor>E. Moraitou, i-Lab, University of the Aegean</dc:contributor>
        <dc:contributor>K. Kotis, i-Lab, University of the Aegean</dc:contributor>
        <dc:contributor>S. Angelis, i-Lab, University of the Aegean</dc:contributor>
        <vann:preferredNamespacePrefix xml:lang="en">dront</vann:preferredNamespacePrefix>
        <rdfs:comment xml:lang="en">The aim of the ontology is to representthe knowledge related to drones&apos; semantic trajectories (flights, missions, recordings). Reuses datAcron ontology.</rdfs:comment>
        <rdfs:label xml:lang="en">onto4drone</rdfs:label>
        <owl:versionInfo xml:lang="en">version 1.0.0</owl:versionInfo>
    </owl:Ontology>
    


