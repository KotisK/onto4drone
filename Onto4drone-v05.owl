    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#ConstantRecordingPosition -->

    <owl:Class rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#ConstantRecordingPosition">
        <rdfs:subClassOf rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#RecordingPosition"/>
        <rdfs:subClassOf rdf:resource="http://www.datacron-project.eu/datAcron#TrajectoryPart"/>
        <rdfs:comment xml:lang="en">The spatiotemporal position on which recording -constantly- occurs.</rdfs:comment>
        <rdfs:label xml:lang="en">Constant Recording Position</rdfs:label>
    </owl:Class>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#Drone -->

    <owl:Class rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#Drone">
        <rdfs:subClassOf rdf:resource="http://www.datacron-project.eu/datAcron#MovingObject"/>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
        <rdfs:comment>Any type of unmanned vehicle, aerial or marine (surface or underwater).</rdfs:comment>
        <rdfs:label>Drone</rdfs:label>
    </owl:Class>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#DroneCarryingEquipment -->

    <owl:Class rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#DroneCarryingEquipment">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
        <rdfs:comment xml:lang="en">The equipment carried by the drone for different purposes.</rdfs:comment>
        <rdfs:label xml:lang="en">Drone Carrying Equipment</rdfs:label>
    </owl:Class>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#DroneRecordingSegment -->

    <owl:Class rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#DroneRecordingSegment">
        <rdfs:subClassOf rdf:resource="http://www.datacron-project.eu/datAcron#Segment"/>
        <rdfs:comment>The process of recording which takes place in certain spatiotemporal positions regarding the trajectory of the flying drone.</rdfs:comment>
        <rdfs:label xml:lang="en">Drone Recording Segment</rdfs:label>
    </owl:Class>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#EntityOfRecording -->

    <owl:Class rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#EntityOfRecording">
        <rdfs:comment xml:lang="en">The entity of interest of any kind (object, event, living organism), that the recording activity aims to record.</rdfs:comment>
        <rdfs:label xml:lang="en">Entity Of Recording</rdfs:label>
    </owl:Class>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#EventOfRecording -->

    <owl:Class rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#EventOfRecording">
        <rdfs:subClassOf rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#EntityOfRecording"/>
