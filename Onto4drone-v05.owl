    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#carriesEquipment -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#carriesEquipment">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#Drone"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#DroneCarryingEquipment"/>
        <rdfs:comment xml:lang="en">Specifies the equipment carried by a drone</rdfs:comment>
        <rdfs:label xml:lang="en">carries equipment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#encloses -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#encloses">
        <rdfs:subPropertyOf rdf:resource="http://www.datacron-project.eu/datAcron#hasPart"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.datacron-project.eu/datAcron#Trajectory"/>
        <rdfs:range rdf:resource="http://www.datacron-project.eu/datAcron#Segment"/>
        <rdfs:comment xml:lang="en">Specifies the trajectory that the segments is part of</rdfs:comment>
        <rdfs:label xml:lang="en">encloses</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#equipmentUsedInEvent -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#equipmentUsedInEvent">
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#DroneCarryingEquipment"/>
        <rdfs:range rdf:resource="http://www.datacron-project.eu/datAcron#Event"/>
        <rdfs:comment xml:lang="en">Specifies the equipmnent used in an event</rdfs:comment>
        <rdfs:label xml:lang="en">equipment used in event</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#hasFlight -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#hasFlight">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParticipantIn"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#UAVDrone"/>
        <rdfs:range rdf:resource="http://www.opengis.net/ont/sf#Flight"/>
        <rdfs:label xml:lang="en">has flight</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#hasMission -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#hasMission">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParticipantIn"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#Drone"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#Mission"/>
        <rdfs:comment xml:lang="en">Speifies the mission that the drone is participating in.</rdfs:comment>
        <rdfs:label xml:lang="en">has mission</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#hasTrajectory -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#hasTrajectory">
        <owl:inverseOf rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#trajectoryRefersTo"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.datacron-project.eu/datAcron#MovingObject"/>
        <rdfs:range rdf:resource="http://www.datacron-project.eu/datAcron#Trajectory"/>
        <rdfs:comment xml:lang="en">Specifies the moving object that forms the trajectory.</rdfs:comment>
        <rdfs:label xml:lang="en">has Trajectory</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#includesEquipment -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#includesEquipment">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#Mission"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#DroneCarryingEquipment"/>
        <rdfs:comment xml:lang="en">Specifies the equipment being used in a mission ro a flight</rdfs:comment>
        <rdfs:label xml:lang="en">includes equipment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#includesFlight -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#includesFlight">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasConstituent"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#Mission"/>
        <rdfs:range rdf:resource="http://www.opengis.net/ont/sf#Flight"/>
        <rdfs:comment xml:lang="en">Specifies flightes included in a misson</rdfs:comment>
        <rdfs:label xml:lang="en">includes flight</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#isPartOfSwarm -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#isPartOfSwarm">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#UAVDrone"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#SwarmOfDrones"/>
        <rdfs:label xml:lang="en">is part of swarm</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#produces -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#produces">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#RecordingEvent"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#Record"/>
        <rdfs:comment xml:lang="en">Specifies the record produced by a recornding event</rdfs:comment>
        <rdfs:label xml:lang="en">produces</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#records -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#records">
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#RecordingEvent"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#EntityOfRecording"/>
        <rdfs:comment xml:lang="en">Specifies the entity that is beeing recorded in a recording event</rdfs:comment>
        <rdfs:label xml:lang="en">records</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#trajectoryRefersTo -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#trajectoryRefersTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.datacron-project.eu/datAcron#Trajectory"/>
        <rdfs:range rdf:resource="http://www.datacron-project.eu/datAcron#MovingObject"/>
        <rdfs:comment xml:lang="en">Defines the moving object that the trajectory is referring to.</rdfs:comment>
        <rdfs:label xml:lang="en">trajectory refers to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.co-ode.org/ontologies/ont.owl#clusters -->

    <owl:ObjectProperty rdf:about="http://www.co-ode.org/ontologies/ont.owl#clusters">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart"/>
        <rdfs:domain rdf:resource="http://www.datacron-project.eu/datAcron#TrajectoryCluster"/>
        <rdfs:range rdf:resource="http://www.datacron-project.eu/datAcron#Trajectory"/>
        <rdfs:comment>Defines the trajectories clusetered in a TrajectorCluster</rdfs:comment>
        <rdfs:label xml:lang="en">clusters</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.co-ode.org/ontologies/ont.owl#isPartOfSwarm -->

    <owl:ObjectProperty rdf:about="http://www.co-ode.org/ontologies/ont.owl#isPartOfSwarm">
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#Drone"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#Swarm"/>
        <rdfs:comment>Defines the swarm that the drone is part of</rdfs:comment>
        <rdfs:label>is part of swarm</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.co-ode.org/ontologies/ont.owl#originatesFrom -->

    <owl:ObjectProperty rdf:about="http://www.co-ode.org/ontologies/ont.owl#originatesFrom">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#ProcessedRecord"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#Record"/>
        <rdfs:comment xml:lang="en">Defines the record from which a processed record originates</rdfs:comment>
        <rdfs:label xml:lang="en">originates from</rdfs:label>
    </owl:ObjectProperty>
    


