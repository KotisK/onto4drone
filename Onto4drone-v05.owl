    <!-- http://www.datacron-project.eu/datAcron#TimeStart -->

    <owl:DatatypeProperty rdf:about="http://www.datacron-project.eu/datAcron#TimeStart">
        <rdfs:domain rdf:resource="http://www.datacron-project.eu/datAcron#Period"/>
    </owl:DatatypeProperty>
    


