    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#hasTime -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#hasTime">
        <rdfs:domain rdf:resource="http://www.datacron-project.eu/datAcron#Time"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:label xml:lang="en">has time</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#hasVelocity -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#hasVelocity">
