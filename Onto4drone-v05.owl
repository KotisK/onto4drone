        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">has velocity</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#isActivated -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#isActivated">
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#Drone"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:label xml:lang="en">is activated</rdfs:label>
    </owl:DatatypeProperty>
    


